{
  "name": "jrgen",
  "version": "3.1.1",
  "description": "Generates docs, tests, clients and servers for json-rpc apis.",
  "keywords": [
    "generator",
    "json-rpc",
    "api",
    "doc",
    "test",
    "client",
    "server"
  ],
  "author": "Marco Zernetsch <m.zernetsch@gmail.com>",
  "license": "MIT",
  "bin": "./src/jrgen.js",
  "bugs": "https://github.com/mzernetsch/jrgen/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzernetsch/jrgen.git"
  },
  "scripts": {
    "build:examples": "npm run clear:examples && node ./scripts/create-examples.js",
    "clear:examples": "node ./scripts/clear-examples.js",
    "format": "prettier --write src/**/*.{js,ts,css,scss,json,md}"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "json-schema-ref-parser": "^9.0.9",
    "json-schema-to-typescript": "^13.0.2",
    "mustache": "^4.2.0",
    "prettier": "^3.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "semantic-release": "^21.0.7"
  }
}
